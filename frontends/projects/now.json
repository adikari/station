{
  "name": "projects-frontend",
  "version": 2,
  "build": {
    "env": {}
  },
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    },
    {
      "src": "public/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)(png|xml|txt|ttf|eot|woff)$",
      "headers": {
        "cache-control": "s-maxage=300"
      },
      "dest": "static/$1$2"
    },
    {
      "src": "/favicon.ico",
      "headers": {
        "content-type": "image/x-icon",
        "cache-control": "s-maxage=300"
      },
      "dest": "/favicon.ico"
    },
    {
      "src": "/projects",
      "dest": "/projects"
    },
    {
      "src": "/project/(?<projectId>[^/]+)",
      "dest": "/project?projectId=$projectId"
    }
  ]
}
