{
  "name": "projects-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "jest --verbose",
    "dev": "NODE_ENV=development now dev --listen 3000",
    "start": "NODE_ENV=production now dev --listen 3000",
    "build": "NODE_ENV=production next build",
    "deploy": "now deploy",
    "debug": "ls -la",
    "test:ci": "JEST_JUNIT_OUTPUT_DIR=artifacts/ jest --verbose --coverage --ci --reporters=default --reporters=jest-junit",
    "lint": "eslint './src/**/*.js' './src/**/*.jsx'",
    "lint:fix": "eslint './src/**/*.js' './src/**/*.jsx' --fix"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-source-maps": "^0.0.3",
    "antd": "^3.25.1",
    "less-loader": "^5.0.0",
    "next": "9.1.3",
    "next-compose-plugins": "^2.2.0",
    "next-images": "^1.2.0",
    "next-routes": "^1.4.2",
    "nprogress": "^0.2.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-svg": "^10.0.23",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "less": "^3.10.3",
    "now": "^16.4.4",
    "prettier": "^1.19.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-prettier": "^1.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint './src/**/*.js' --fix",
      "git add ."
    ]
  }
}
